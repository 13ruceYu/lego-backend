{
  "name": "lego-backend",
  "version": "0.0.6",
  "description": "test",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-lego-backend",
    "start-no-daemon": "egg-scripts start --title=egg-server-lego-backend",
    "stop": "egg-scripts stop --title=egg-server-lego-backend",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "upload": "node webpack/uploadToOSS.js"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "ali-oss": "^6.18.1",
    "axios": "^0.21.4",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "egg": "^2.6.1",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-oss": "^3.1.0",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2.6.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lego-components": "^0.1.7",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.20",
    "mongoose-sequence": "^5.3.1",
    "nanoid": "^3.3.6",
    "stream-wormhole": "^2.0.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.9",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.200",
    "@types/mocha": "^2.2.40",
    "@types/node": "^16.18.58",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "tslib": "^1.9.0",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "viking zhang",
  "license": "MIT"
}
